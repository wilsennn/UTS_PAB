<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="16dp"
    tools:context=".ResultActivity"
    >
    <Button
        android:id="@+id/tv_uts"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="<UTS"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="@color/black"
        android:background=""
        android:backgroundTint="@color/white"
        />


    <TextView
        android:id="@+id/header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="70dp"
        android:textAlignment="center"
        android:text="KONFIRMASI PENDAFTARAN"
        android:textSize="18sp"
        android:textStyle="bold"
        />
    <TextView
        android:id="@+id/tv_tampil_nama"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:textSize="14sp"
        android:textStyle="bold"
        android:layout_below="@id/header"

        />

    <TextView
        android:id="@+id/tv_tampil_nomor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_tampil_nama"
        android:layout_marginStart="5dp"
        android:layout_marginTop="20dp"

        android:layout_marginEnd="5dp"
        android:layout_marginBottom="5dp"
        android:textSize="14sp"
        android:textStyle="bold" />


    <TextView
        android:id="@+id/informasi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_tampil_nomor"
        android:layout_marginTop="57dp"
        android:text="Informasi Pendaftaran Dari :"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tv_tampil_info"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/informasi"
        android:layout_marginTop="5dp"
        android:textSize="14sp"
        android:textStyle="bold"
        />


</RelativeLayout>
